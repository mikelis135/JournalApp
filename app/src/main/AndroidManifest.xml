<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alcwithgoogle.journalapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/splashscreen"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activity.WelcomeActivity"
            android:label="@string/myjournals"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:label="@string/newjournal" android:name=".Activity.EditJounalActivity" android:parentActivityName=".Activity.MainActivity"></activity>
        <activity android:label="@string/signin" android:name=".Activity.SigninActivity"></activity>
        <activity android:label="@string/journal" android:name=".Activity.MainActivity" android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>